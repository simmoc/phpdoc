# ======================================
# Ragnar Fiery Dig Point Nginx Configure
# ======================================

# where is the Performance log save
fastcgi_param  RAGNAR_LOGPATH "/var/log/fiery/";

# Which IDC this Server
# Number Range 0 to 3
fastcgi_param  RAGNAR_IDC "0";

# This Server IP
fastcgi_param  RAGNAR_IP  "127.0.0.1";
